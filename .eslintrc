{
    "env": {
      "node": true,
      "es2021": true,
      "jest": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "semi": ["error", "always"],
      "no-console": "warn",
      "no-undef": "error",
      "no-multi-spaces": "error",
      "no-var": "error"
    }
}  